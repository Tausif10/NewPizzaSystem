
# An example controller showing a sample home page
GET     /                           controllers.HomeController.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file              controllers.Assets.versioned(path="/public", file: Asset)

GET    /home                       controllers.HomeController.home

POST   /search                     controllers.SearchController.search

POST   /filter                     controllers.FilterController.filter

GET    /addToCart                  controllers.AddToCartController.addToCart

POST   /getBill                    controllers.BillController.bill

POST   /payBill                    controllers.BillController.payBill

GET   /cancel                     controllers.OrderCancelController.cancel
